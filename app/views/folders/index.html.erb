<h1>フォルダ一覧</h1>
<div class="folder-table">
  <table>
    <thead>
      <tr>
        <th class="name">名前</th>
        <th class="date">最終更新日</th>
      </tr>
    </thead>
    <tbody>
    <% folder = {name: "テスト名１", date: "2021-01-27"} %>
    <% i = 0 %>
    <% while i <= 30 do %>
      <tr>
        <td class="name-value"><%= folder[:name] %></td>
        <td class="date-value"><%= folder[:date] %></td>
      </tr>
    <% i += 1 %>
    <% end %>
    </tbody>
  </table>
</div>
